{
    "manifest_version": 2,
    "name": "Nagfree",
    "description": "Makes a few sites a little better",
    "version": "0.1",
    "permissions" : ["tabs", "<all_urls>"],
    "web_accessible_resources" : [
        "*.js",
        "lib/*.js",
        "scripts.json",
        "scripts/*.js"
    ],
    "background" : {
        "scripts" : ["background.js"],
        "persistent" : false
    }
}