{
    "manifest_version": 2,
    "name": "Cite Delpher on Wiki",
    "description": "Adds citation wikitext to the \"share\" options on Delpher.nl, the Dutch language newspaper archive",
    "version": "0.1",
    "permissions" : [
        "activeTab"
    ],
     "optional_permissions": [
      "topSites",
      "http://www.delpher.nl/*"
    ],
    "web_accessible_resources" : [
        "*.js",
        "scripts/*.js"
    ],
    "background" : {
        "page" : "background.html"
    }
}