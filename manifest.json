{
    "manifest_version": 2,
    "name": "Nagfree",
    "description": "Makes a few sites a little better",
    "version": "0.2",
    "permissions" : [
        "tabs",
        "webNavigation",
        "<all_urls>"
    ],
    "web_accessible_resources" : [
        "*.js",
        "lib/*.js",
        "scripts.json",
        "scripts/*.js"
    ],
    "background" : {
        "page" : "background.html"
    }
}