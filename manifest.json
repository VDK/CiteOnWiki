{
    "manifest_version": 2,
    "name": "Cite on Wiki",
    "description": "Adds citation wikitext to the \"share\" options",
    "version": "0.1",
    "permissions" : [
        "activeTab",
        "webNavigation",
        "<all_urls>"
    ],
    "web_accessible_resources" : [
        "*.js",
        "scripts/*.js"
    ],
    "background" : {
        "page" : "background.html"
    }
}